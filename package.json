{
  "name": "movetree",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "cd packages/ui && npm run dev",
    "build": "cd packages/ui && npm run build",
    "lint": "cd packages/ui && npm run lint",
    "preview": "cd packages/ui && npm run preview",
    "move:test": "cd packages/move && sui move test",
    "move:build": "cd packages/move && sui move build",
    "move:publish": "cd packages/move && sui move publish",
    "deploy:walrus": "tsx scripts/publish-walrus.ts",
    "link:suins": "tsx scripts/link-suins.ts",
    "install:all": "npm install && cd packages/ui && npm install"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "js-yaml": "^4.1.0",
    "tsx": "^4.7.0"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
  "dependencies": {
    "@mysten/dapp-kit": "^0.19.6",
    "@mysten/enoki": "^0.12.8",
    "@mysten/sui": "^1.43.1",
    "@noble/hashes": "^2.0.1",
    "@tanstack/react-query": "^5.90.5",
    "@web3-storage/w3up-client": "^17.3.0",
    "microlink": "^0.3.0",
    "plausible-tracker": "^0.3.9",
    "zod": "^4.1.12"
  }
}
