{
  "name": "movetree",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "cd packages/ui && npm run dev",
    "build": "cd packages/ui && npm run build",
    "lint": "cd packages/ui && npm run lint",
    "preview": "cd packages/ui && npm run preview",
    "move:test": "cd packages/move && sui move test",
    "move:build": "cd packages/move && sui move build",
    "move:publish": "cd packages/move && sui move publish",
    "deploy:walrus": "tsx scripts/publish-walrus.ts",
    "link:suins": "tsx scripts/link-suins.ts",
    "install:all": "npm install && cd packages/ui && npm install"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "js-yaml": "^4.1.0",
    "@types/js-yaml": "^4.0.9"
  }
}
